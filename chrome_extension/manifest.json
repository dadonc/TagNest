{
  "manifest_version": 3,
  "name": "HRB Extension",
  "description": "Save websites to HRB",
  "version": "1.0",
  "action": {
    "default_popup": "popup/popup.html"
  },
  "background": {
    "service_worker": "scripts/background.js"
  },

  "permissions": ["pageCapture", "tabs", "activeTab", "scripting"]
}
